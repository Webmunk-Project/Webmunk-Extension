{
  "name": "Webmunk", 
  "short_name": "Webmunk", 
  "icons": {
      "256": "images/icon-256.png", 
      "128": "images/icon-128.png", 
      "48": "images/icon-48.png",
      "32": "images/icon-32.png", 
      "16": "images/icon-16.png"
  }, 
  "description": "Performs web experiments for research studies.", 
  "background": {
    "service_worker": "js/app/background_loader.js"
  }, 
  "version": "0.12", 
  "manifest_version": 3, 
  "action": {
    "default_icon": {
      "256": "images/icon-256.png", 
      "128": "images/icon-128.png", 
      "48": "images/icon-48.png",
      "32": "images/icon-32.png", 
      "16": "images/icon-16.png"
	}    
  }, 
  "permissions": [
  	"scripting",
    "storage", 
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
